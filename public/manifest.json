{
  "manifest_version": 3,
  "name": "AlumniConnectAI",
  "version": "1.0",
  "description": "Automate alumni outreach with personalized emails",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*.upenn.edu/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/AlumniConnectIcon.png"
    
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
  "16": "assets/AlumniConnectIcon.png",
  "48": "assets/AlumniConnectIcon.png",
  "128": "assets/AlumniConnectIcon.png"
  }
}
